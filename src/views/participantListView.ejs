                                        <a href="/Participants/<%=participants[i]._id %>" class="dmbutton a2" data-animate="fadeInUp"><i class="fa fa-link"></i></a>

                                    </div>
                                    <!-- he bg -->
                                </div>
                                <!-- he view -->
                            </div>
                            <!-- he wrap -->
                            <h4><%=participants[i].firstName %> <%=participants[i].lastName %></h4>

                        </div>
                        <! --/col-lg-3 -->
                        <%}%>

                </div>
                <! --/row -->
            </div>
            <! --/container -->
            
